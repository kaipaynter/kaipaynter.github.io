  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "setup": "npm install",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "branchCleanup": "git branch -D gh-pages",
    "branchSetup": "git checkout -b gh-pages",
    "deploy": "gatsby build && gh-pages -d public",
    "reset": "git checkout master",
    "deploy:full": "npm run branchCleanup && npm run branchSetup && npm run deploy && npm run reset"
  },
  "devDependencies": {
    "gh-pages": "^3.2.3",
    "prettier": "^2.4.1",
    "rimraf": "^3.0.2"
