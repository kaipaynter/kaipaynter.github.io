  ],
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "setup": "npm install",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1",
    "branchCleanup": "git branch -D gh-pages",
    "branchSetup": "git checkout -b gh-pages",
    "deploy": "gatsby build && gh-pages -d public",
    "reset": "git checkout main",
    "deploy:full": "npm run branchCleanup && npm run branchSetup && npm run deploy && npm run reset"
  },
  "devDependencies": {
    "gh-pages": "^5.0.0",
    "prettier": "^3.0.3",
    "rimraf": "^5.0.0",
    "sass": "^1.56.1"
